{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "MimirStorage",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::mimir-bucket-67",
                "arn:aws:s3:::mimir-bucket-67/*",
                "arn:aws:s3:::mimir-bucket-blocks-67",
                "arn:aws:s3:::mimir-bucket-blocks-67/*"
            ]
        }
    ]
}